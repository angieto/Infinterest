@{
    ViewData["Title"] = "Dashboard";
}

@section css {
    <link href="@Url.Content("~/css/dashboardstyle.css")" rel="stylesheet" type="text/css" />
}

<div id="dashboardcontainer">
    <nav>
        <a href="#"><img src="../images/white-nosub-inflogo.png" alt="logo" class="whitelogo"></a>
        <div class="navlinks">
            <a class="nav-link" href="#">About</a>
            <a class="nav-link" href="#">Messaging</a>
            <a class="nav-link" href="/profile">Profile</a>
            <a class="nav-link" href="/logoff">Sign out</a>
