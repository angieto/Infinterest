                <a class="nav-link" href="broker-registration">Broker Sign up</a>
                <a class="nav-link" href="registration">Buyer/Seller Sign up</a>
                <a class="nav-link login">Log in</a>
            </div>
        </nav>
        <div class="searchbox">
            <h4>Search Upcoming Open House Events.</h4>
            <form>
                <div class="form-group whereinput">
                  <label for="zipcode">Where</label>
                  <input type="text" class="form-control"placeholder="Zip Code">
                </div>
                <div class="form-group">
                    <label for="zipcode" class="when">When</label>
                    <input type="date" class="form-control daterange">
                    <label>to</label>
                    <input type="date" class="form-control daterange">
                </div>
                <div class="form-group">
                    <label for="price" class="when">Price Range</label>
                    <input type="number" class="form-control daterange" min="10000" max="100000000" value="10000">
                    <label>to</label>
                    <input type="number" class="form-control daterange" min="10000" max="100000000" value="100000000">
                </div>
                <button class="btn btn-primary" type="submit">Search</button>
            </form>
        </div>
            <div class="loginbox">
                <form onsubmit="gotodashboard()">
                    <i class="fas fa-times-circle"></i>
                    <div class="form-group logformgroup">
                      <input type="text" class="form-control logformcon" placeholder="Email">
                    </div>
                    <div class="form-group logformgroup">
                      <input type="text" class="form-control logformcon" placeholder="Password">
                    </div>
                    <a class="btn btn-primary logbutton" href="dashboard">Log in</a>
                </form>
        </div>
        <div class="carrot">
            <i class="fas fa-chevron-down"></i>
        </div>
    </div>
